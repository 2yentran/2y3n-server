{
	"name": "2yen-server",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "npm run prod",
		"server": "babel-node ./src/bin/www",
		"server:prod": "node ./dist/bin/www",
		"dev": "cross-env NODE_ENV=development npm-run-all server",
		"clean": "rimraf dist",
		"build": "babel ./src --out-dir dist",
		"prod": "cross-env NODE_ENV=production npm-run-all clean build server:prod",
		"watch": "nodemon",
		"debug": "nodemon"
	},
	"dependencies": {
		"@okta/jwt-verifier": "^1.0.0",
		"axios": "^0.19.0",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"cross-env": "^6.0.3",
		"debug": "~2.6.9",
		"dotenv": "^8.2.0",
		"express": "~4.16.1",
		"mongoose": "^5.7.7",
		"morgan": "~1.9.1",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.6.4",
		"@babel/core": "^7.6.4",
		"@babel/node": "^7.6.3",
		"@babel/preset-env": "^7.6.3",
		"nodemon": "^1.19.4"
	}
}
