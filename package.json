{
	"name": "2y3n-server",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "npm run prod",
		"server": "babel-node ./src/bin/www",
		"server:prod": "node ./dist/bin/www > stdout.txt 2> stderr.txt &",
		"dev": "cross-env NODE_ENV=development npm-run-all server",
		"clean": "rimraf dist",
		"build": "babel ./src --out-dir dist",
		"prod": "cross-env NODE_ENV=production npm-run-all clean build server:prod",
		"watch": "nodemon"
	},
	"dependencies": {
		"@babel/runtime": "^7.6.3",
		"@okta/jwt-verifier": "^4.0.1",
		"axios": "^1.7.9",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"cross-env": "^6.0.3",
		"debug": "~2.6.9",
		"dotenv": "^8.2.0",
		"express": "^4.21.2",
		"mongoose": "^6.13.5",
		"morgan": "~1.9.1",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.20.7",
		"@babel/core": "^7.6.4",
		"@babel/node": "^7.6.3",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/preset-env": "^7.6.3",
		"nodemon": "^3.1.7"
	}
}
